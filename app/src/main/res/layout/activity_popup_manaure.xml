<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dp"
    android:layout_gravity="center"
    android:layout_height="300dp"
    tools:context=".Presentacion.PopupMunicipio.Popup_manaure"
    android:padding="16dp"
    android:background="@color/popup">
    <ImageView
        android:id="@+id/foto"
        android:layout_width="132dp"
        android:layout_height="84dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"

        android:layout_marginTop="25dp"
        android:layout_marginEnd="71dp"
        android:layout_marginRight="71dp"
        android:src="@drawable/manaure" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="1dp"
        android:gravity="center_horizontal"
        android:layout_marginEnd="86dp"
        android:layout_marginRight="86dp"
        android:text="Manaure"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:textStyle="bold" />

    <TextView
        android:layout_width="252dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="105dp"
        android:gravity="center_horizontal"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:text="Descripcion"
        android:textSize="13sp"
        android:textColor="@color/black"
        android:textStyle="bold" />

    <TextView
        android:layout_width="271dp"
        android:layout_height="59dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="122dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:text="@string/DescripcionManaure"
        android:textSize="10sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="189dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="175dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:gravity="center_horizontal"
        android:text="Historia"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="275dp"
        android:layout_height="101dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        android:text="@string/Historia_Manaure"
        android:textSize="10sp"
        android:textStyle="bold" />

</RelativeLayout>